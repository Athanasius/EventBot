[% INCLUDE header.tt %]

<div class="event">
  Name: [% pub.name %]<br/>
  Region: [% pub.region %]<br/>
  Address: [% pub.street_address %]<br/>
  URL: <a href="[% pub.info_uri %]">[% pub.info_uri %]</a><br/>
  Endorsed by:
  <ul>
    [% FOREACH nom IN pub.nominees %]
    <li><a href="/people/view/[% nom.name %]">[% nom.name %]</a></li>
    [% END %]
  </ul>

</div>

[% INCLUDE footer.tt %]
